{
  "name": "@edx/frontend-app-enterprise-checkout",
  "version": "0.1.0",
  "description": "B2B self-service checkout MFE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-app-enterprise-checkout.git"
  },
  "browserslist": [
    "extends @edx/browserslist-config"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "i18n_extract": "fedx-scripts formatjs extract",
    "check-types": "tsc --noEmit",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx --ext .ts --ext .tsx .",
    "lint:fix": "npm run lint -- --fix",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests",
    "test:watch": "npm run test -- --watch",
    "snapshot": "npm run test -- -- -u",
    "serve": "fedx-scripts serve"
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/edx/frontend-app-enterprise-checkout#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/edx/frontend-app-enterprise-checkout/issues"
  },
  "dependencies": {
    "@edx/brand": "npm:@edx/brand-edx.org@^2.1.3",
    "@edx/frontend-component-header": "^5.8.3",
    "@edx/frontend-platform": "^8.3.1",
    "@edx/typescript-config": "^1.1.0",
    "@hookform/resolvers": "^4.1.3",
    "@openedx/frontend-slot-footer": "^1.1.0",
    "@openedx/paragon": "^22.15.3",
    "classnames": "^2.5.1",
    "core-js": "^3.41.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.54.2",
    "react-redux": "^7.2.9",
    "react-router": "^6.29.0",
    "react-router-dom": "^6.29.0",
    "redux": "^4.2.1",
    "slugify": "^1.6.6",
    "zod": "^3.24.2",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@edx/browserslist-config": "^1.1.1",
    "@openedx/frontend-build": "^14.3.2",
    "eslint-import-resolver-typescript": "^3.8.5",
    "jest": "^29.7.0",
    "tsconfig-paths-webpack-plugin": "^4.2.0"
  }
}
